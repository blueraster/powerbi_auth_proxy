<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>My project</title>
    <parser>
        <target>output</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>docs/api-docs</target>
    </transformer>
    <logging>
        <level>warn</level>
    </logging>
    <files>
        <directory>src</directory>
        <ignore>src/assets/*</ignore>
    </files>
</phpdoc>
